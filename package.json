{
	"name": "n8n-nodes-scrapingfish",
	"version": "0.1.0",
	"description": "n8n community node for Scrapingfish web scraping API",
	"license": "MIT",
	"homepage": "https://www.scrapingfish.com/",
	"keywords": [
		"n8n-community-node-package",
		"n8n-node",
		"n8n",
		"scrapingfish",
		"scraping",
		"web scraping"
	],
	"author": {
		"name": "mwojciechowski",
		"email": "maciej.wojciechowski@narf.ai"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Narf-AI/n8n-scrapingfish"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/ScrapingfishApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Scrapingfish/Scrapingfish.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
